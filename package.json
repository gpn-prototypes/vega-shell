{
  "name": "@gpn-prototypes/vega-shell",
  "author": "csssr",
  "license": "UNLICENSED",
  "version": "0.0.1",
  "engines": {
    "node": "^12.16.2",
    "yarn": "^1.22.4"
  },
  "scripts": {
    "dev": "webpack-dev-server --mode=development --port 9000 --env.isLocal=true",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "coverage": "jest --all --coverage",
    "format": "prettier --write {src,types,scripts}/**/*.{json,{j,t}s{,x},css,}",
    "build": "webpack --mode=production",
    "lint": "npm-run-all lint:*",
    "lint:fix": "npm-run-all format lint:fix:*",
    "lint:ts": "eslint {src,types}/**/*.ts{,x}",
    "lint:fix:ts": "eslint {src,types}/**/*.ts{,x} --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./node_modules/@gpn-prototypes/frontend-configs/git/commitizen.js"
    }
  },
  "dependencies": {
    "@types/jest": "^25.2.1",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.15.1",
    "faker": "^5.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jsondiffpatch": "^0.4.1",
    "single-spa": "^5.3.1",
    "single-spa-layout": "1.0.0-beta.11"
  },
  "devDependencies": {
    "@gpn-prototypes/frontend-configs": "^1.2.12",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/systemjs": "^6.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "commitizen": "^4.2.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "cz-customizable": "^6.3.0",
    "dotenv": "^8.2.0",
    "eslint-config-ts-important-stuff": "^1.0.0",
    "fetch-mock": "^9.10.7",
    "html-webpack-plugin": "^4.0.4",
    "husky": "^4.2.3",
    "jest": "^26.4.2",
    "jest-cli": "^25.2.7",
    "lint-staged": "^10.4.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "ts-config-single-spa": "^1.7.0",
    "typescript": "^4.0.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-config-single-spa-ts": "^1.4.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "worker-plugin": "^5.0.0"
  }
}
