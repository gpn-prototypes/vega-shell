{
  "name": "@gpn-prototypes/vega-shell",
  "author": "gpn-prototypes",
  "license": "UNLICENSED",
  "version": "0.0.1",
  "engines": {
    "node": "^12.16.2",
    "yarn": "^1.22.4"
  },
  "scripts": {
    "dev": "yarn generate:dev:types && webpack-dev-server --mode=development --port 9000 --env.isLocal=true --env.port=9000",
    "generate:types": "graphql-codegen --config codegen.yml",
    "generate:dev:types": "yarn generate:types --require dotenv/config",
    "generate:dev:types:watch": "yarn generate:dev:types --watch",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --mode=production",
    "lint:fix:ts": "eslint {src,types}/**/*.ts{,x} --fix",
    "format": "prettier --write '{src,types,scripts}/**/*.{json,{j,t}s{,x},css,}'",
    "coverage": "jest --all --coverage",
    "coverage:changes": "jest --coverage --onlyChanged",
    "lint:ts": "eslint --ext=.ts,.tsx .",
    "lint:ts:fix": "eslint --ext=.ts,.tsx . --fix",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:css:fix": "stylelint ./src/**/*.css --fix",
    "lint": "yarn run lint:ts && yarn run lint:css",
    "lint:fix": "yarn run lint:ts:fix && yarn run lint:css:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./node_modules/@gpn-prototypes/frontend-configs/git/commitizen.js"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@gpn-prototypes/frontend-configs": "^1.3.2",
    "@gpn-prototypes/vega-ui": "^3.1.10",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/named-operations-object": "^1.17.9",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/schema-ast": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-tools/mock": "^7.0.0",
    "@graphql-tools/schema": "^7.1.3",
    "@graphql-tools/utils": "^7.2.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.7.0",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/single-spa-react": "^2.12.0",
    "@types/systemjs": "^6.1.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack-env": "^1.15.1",
    "apollo-server": "^2.19.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "cross-fetch": "^3.0.6",
    "dotenv": "^8.2.0",
    "faker": "^5.3.1",
    "fetch-mock": "^9.10.7",
    "file-loader": "^6.2.0",
    "final-form": "^4.20.1",
    "graphql": "^15.3.0",
    "html-webpack-plugin": "^4.0.4",
    "import-map-overrides": "^2.1.0",
    "jest": "^26.6.3",
    "jest-cli": "^25.2.7",
    "jsondiffpatch": "^0.4.1",
    "postcss": "^8.2.10",
    "postcss-loader": "^4.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-final-form": "^6.5.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "single-spa": "^5.8.0",
    "single-spa-layout": "1.1.5",
    "single-spa-react": "^3.2.0",
    "systemjs": "^6.7.1",
    "ts-config-single-spa": "^1.7.0",
    "typescript": "^4.0.3",
    "uuid": "^8.3.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-config-single-spa-ts": "^1.4.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-import-map-plugin": "^1.0.0-alpha0",
    "webpack-merge": "^4.2.2",
    "worker-plugin": "^5.0.0"
  },
  "devDependencies": {
    "commitizen": "^4.2.1",
    "concurrently": "^5.1.0",
    "cz-customizable": "^6.3.0",
    "husky": "^4.2.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.4.0"
  }
}
